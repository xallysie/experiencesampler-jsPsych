<?xml version='1.0' encoding='utf-8'?>
<widget id="edu.princeton.dailyidentitystudy" version="1.2.0" android-versionCode="20" ios-CFBundleVersion="1.2.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Daily Studies About the Self</name>
    <description>A smartphone app developed to conduct experience-sampling smartphone studies using the jsPsych framework.</description>
    <author email="sally.yj.xie@gmail.com" href="https://sallyxie.org">
        Sally Xie
    </author>
    <content src="index.html" />
     <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="AndroidInsecureFileModeEnabled" value="true" />
        <!--
        ldpi    : 36x36 px
        mdpi    : 48x48 px
        hdpi    : 72x72 px
        xhdpi   : 96x96 px
        xxhdpi  : 144x144 px
        xxxhdpi : 192x192 px
        -->
        <icon src="www/img/icon.png" density="ldpi" />
        <icon src="www/img/icon.png" density="mdpi" />
        <icon src="www/img/icon.png" density="hdpi" />
        <icon src="www/img/icon.png" density="xhdpi" />
        <icon src="www/img/icon.png" density="xxhdpi" />
        <icon src="www/img/icon.png" density="xxxhdpi" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon src="www/img/icon.png" width="192" height="192"/>
        <!-- iOS 8.0+ -->
        <!-- iPhone 6 Plus  -->
        <icon src="www/img/icon.png" width="180" height="180" />
        <!-- iOS 7.0+ -->
        <!-- iPhone / iPod Touch  -->
        <icon src="www/img/icon.png" width="60" height="60" />
        <icon src="www/img/icon.png" width="120" height="120" />
        <!-- iPad -->
        <icon src="www/img/icon.png" width="76" height="76" />
        <icon src="www/img/icon.png" width="152" height="152" />
        <!-- Spotlight Icon -->
        <icon src="www/img/icon.png" width="40" height="40" />
        <icon src="www/img/icon.png" width="80" height="80" />
        <!-- iOS 6.1 -->
        <!-- iPhone / iPod Touch -->
        <icon src="www/img/icon.png" width="57" height="57" />
        <icon src="www/img/icon.png" width="114" height="114" />
        <!-- iPad -->
        <icon src="www/img/icon.png" width="72" height="72" />
        <icon src="www/img/icon.png" width="144" height="144" />
        <!-- iPhone Spotlight and Settings Icon -->
        <icon src="www/img/icon.png" width="29" height="29" />
        <icon src="www/img/icon.png" width="58" height="58" />
        <!-- iPad Spotlight and Settings Icon -->
        <icon src="www/img/icon.png" width="50" height="50" />
        <icon src="www/img/icon.png" width="100" height="100" />
        <!-- iPad Pro -->
        <icon src="www/img/icon.png" width="167" height="167" />
    </platform>
</widget>
